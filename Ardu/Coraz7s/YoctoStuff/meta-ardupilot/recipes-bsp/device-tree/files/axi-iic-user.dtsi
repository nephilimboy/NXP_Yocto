/{
    axi {
        ethernet@e000b000 {
            phy-handle = <&ethernet_phy>;
            local-mac-address = [00 18 3E 04 C7 B0];
            // /delete-property/ enet-reset;
        /*	phy-reset-gpio = <&gpio0 GPIO_ACTIVE_LOW>;
            phy-reset-active-low;	
            phy-reset-duration = <10>;  */
            ethernet_phy: ethernet-phy@1 { /* rtl8211e-vl */
                reg = <1>;
                device_type = "ethernet-phy";
            };
        };

        // Disabeling the write ptotection on SD card so the rootFS can be eddited
        mmc@e0100000 {
            disable-wp;
            xlnx,has-wp = <0x00>;
        };

        usb_phy0: phy0@e0002000 {
            compatible = "ulpi-phy";
            #phy-cells = <0>;
            reg = <0xe0002000 0x1000>;
            view-port = <0x0170>;
            drv-vbus;
	    };
    };


    pl-bus {
        i2c@41600000 {
	        status = "okay";
                clock-frequency = <400000>;
	};

	axi_quad_spi@41e00000 {
		status = "okay";
                is-decoded-cs = <0>;
                xlnx,spi-mode = <0x03>;
                spidev@0 {
                  //compatible = "spidev";
                  // STUPID BUG
                  compatible = "rohm,dh2228fv";
                  reg = <0>;
                  spi-max-frequency = <10000000>; /* (10 MHz) */
                  status = "okay";
                };
	};

    };
};


&usb0 {	
	dr_mode = "host";
	usb-phy = <&usb_phy0>;
};
&amba {
	ptm@f889d000 {
		cpu = <&cpu0>;
	};
};
